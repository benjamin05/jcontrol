<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="mx.com.lux.control.trabajos.data.vo.JbDev" table="jb_dev">
		<id name="id" column="id_dev" type="int">
			<generator class="sequence">
				<param name="sequence">jb_dev_id_dev_seq</param>
			</generator>
		</id>
		<property name="factura" column="factura" type="string" length="7" />
		<property name="sucursal" column="sucursal" type="string" length="5" />
		<property name="apartado" column="apartado" type="string" />
		<property name="idViaje" column="id_viaje" type="string" length="3" />
		<property name="documento" column="documento" type="string" />
		<property name="armazon" column="arm" type="string" length="15" />
		<property name="color" column="col" type="string" length="20" />
		<property name="fechaEnvio" column="fecha_envio" type="java.util.Date" length="13" />
		<property name="fecha" column="fecha" type="java.sql.Timestamp" length="35" />
		<property name="idMod" column="id_mod" type="string" length="13" />
		<property name="rx" column="rx" type="boolean" />
	</class>
</hibernate-mapping>