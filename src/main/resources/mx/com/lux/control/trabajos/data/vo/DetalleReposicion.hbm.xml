<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="mx.com.lux.control.trabajos.data.vo.DetalleReposicion" schema="public" table="repo_det">
        <composite-id>
            <key-property name="factura" column="factura" type="string"/>
            <key-property name="ojo" column="ojo" type="string"/>
            <key-property name="tipo" column="tipo" type="string"/>
        </composite-id>
        <property name="fecha" column="fecha" type="date"/>
        <property name="numeroOrden" column="num_orden" type="integer"/>
        <property name="idSucursal" column="suc" type="string"/>
        <property name="valorAnterior" column="v_old" type="string"/>
        <property name="valorNuevo" column="v_new" type="string"/>
        <property name="campo" column="campo" type="string"/>
    </class>
</hibernate-mapping>