<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="mx.com.lux.control.trabajos.data.vo.JbLlamada" table="jb_llamada">
        <id name="rx" column="rx" type="string">
            <generator class="assigned"/>
        </id>
        <property name="numLlamada" column="num_llamada" type="integer"/>
        <property name="fecha" column="fecha" type="timestamp" length="35"/>
        <property name="estado" column="estado" type="string" length="5"/>
        <property name="contesto" column="contesto" type="string" length="40"/>
        <property name="empAtendio" column="emp_atendio" type="string" length="5"/>
        <property name="empLlamo" column="emp_llamo" type="string" length="5"/>
        <property name="tipo" column="tipo" type="string" length="20"/>
        <property name="obs" column="obs" type="string"/>
        <property name="grupo" column="grupo" type="boolean"/>
        <property name="idMod" column="id_mod" type="string" length="13"/>
    </class>
</hibernate-mapping>
