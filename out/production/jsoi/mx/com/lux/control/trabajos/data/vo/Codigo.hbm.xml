<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="mx.com.lux.control.trabajos.data.vo.Codigo" table="codigoxx" schema="public">
        <composite-id>
            <key-property name="idEstado" column="id_estado" type="string" length="2" />
            <key-property name="idLocalidad" column="id_localidad" type="string" length="3" />
            <key-property name="codigo" column="codigo" type="string" length="5" />
            <key-property name="usuario" column="usuario" type="string" length="60" />
		</composite-id>
		<property name="clase" column="clase" type="string" length="2" />
		<property name="ciudad" column="ciudad" type="string" length="2" />
		<property name="reparto" column="reparto" type="character" length="1" />
		<property name="servicios" column="servicios" type="character" length="1" />
		<property name="oficina" column="oficina" type="string" length="5" />
		<property name="asentamien" column="asentamien" type="string" length="2" />
		<property name="actualiza" column="actualiza" type="integer" />
		<property name="cor" column="cor" type="string" length="5" />
    </class>
</hibernate-mapping>
