<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="mx.com.lux.control.trabajos.data.vo">
	<class name="Reimpresion" table="reimpresion" schema="public">
		<composite-id>
			<key-property name="idNota" type="string" column="id_nota" />
			<key-property name="fecha" type="timestamp" column="fecha" />
		</composite-id>
		<property name="nota" type="string" column="nota" />
		<property name="empleado" type="string" column="empleado" />
		<property name="factura" type="string" column="factura" />
	</class>
</hibernate-mapping>
