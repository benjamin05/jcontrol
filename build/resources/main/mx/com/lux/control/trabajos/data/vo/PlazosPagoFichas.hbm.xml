<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="mx.com.lux.control.trabajos.data.vo.PlazosPagoFichas" table="plazos_pago_fichas" schema="public">
        <id name="idPlazo" type="java.lang.Integer">
            <column name="id_plazo"/>
            <generator class="sequence">
                <param name="sequence">plazos_pago_fichas_id_plazo_seq</param>
            </generator>
        </id>
        <property name="plazo" type="java.math.BigDecimal" column="plazo"/>
        <property name="tipoVenta" type="string" column="tipo_venta"/>
    </class>
</hibernate-mapping>
