<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="mx.com.lux.control.trabajos.data.vo.TipoPago" table="tipo_pago" schema="public">
		<id name="id" column="id_pago" type="string" length="5" />
		<property name="descripcion" column="descripcion" type="string"  />
		<property name="tipoSoi" column="tipo_soi" type="string" length="2" />
		<property name="tipoCon" column="tipo_con" type="string" length="5" />
		<property name="f1" column="f1" type="string" length="20" />
		<property name="f2" column="f2" type="string" length="20" />
		<property name="f3" column="f3" type="string" length="20" />
		<property name="f4" column="f4" type="string" length="20" />
		<property name="f5" column="f5" type="string" length="20" />
	</class>
</hibernate-mapping>
