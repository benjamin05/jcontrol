<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="mx.com.lux.control.trabajos.data.vo.JbTrack" table="jb_track" dynamic-insert="true">
        <composite-id>
            <key-property name="rx" column="rx" type="text" />
            <key-property name="fecha" column="fecha" type="timestamp" />
        </composite-id>
        <property name="estado" column="estado" type="string" length="5" />
        <property name="idViaje" column="id_viaje" type="string" length="3" />
        <property name="idMod" column="id_mod" type="string" length="13" />
        <property name="empleado" column="emp" type="string" length="5" />
        <property name="observaciones" column="obs" type="text" />
    </class>
</hibernate-mapping>
