<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="mx.com.lux.control.trabajos.data.vo.JbEstado" table="jb_edos">
        <id name="idEdo" type="java.lang.String">
            <column name="id_edo" length="5"/>
            <generator class="assigned"/>
        </id>
        <property name="llamada" type="java.lang.String">
            <column name="llamada" length="5"/>
        </property>
        <property name="descr" type="string">
            <column name="descr"/>
        </property>
    </class>
</hibernate-mapping>
