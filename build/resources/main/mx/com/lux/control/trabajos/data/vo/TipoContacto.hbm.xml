<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="mx.com.lux.control.trabajos.data.vo.TipoContacto" table="tipo_contacto">
		<comment>catalogo de tipo de contacto</comment>
		<id name="idTipoContacto" column="id_tipo_contacto" type="int">
			<generator class="sequence">
				<param name="sequence">tipo_contacto_id_tipo_contacto_seq</param>
			</generator>
		</id>
		<property name="descripcion" column="descripcion" type="string" />
	</class>
</hibernate-mapping>
